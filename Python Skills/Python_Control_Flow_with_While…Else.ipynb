      "source": [
        "i = 4\n",
        "\n",
        "while i > 0:\n",
        "    print(i)\n",
        "    if i == 2:\n",
        "        break\n",
        "    i -= 1\n",
        "else:\n",
        "    print('no break')\n",
        "    print(i)"
      ],
