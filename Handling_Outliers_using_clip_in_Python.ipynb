      "source": [
        "import pandas as pd\n",
        "\n",
        "data = {'col0': [10,-2,0,-1,4]}\n",
        "\n",
        "df = pd.DataFrame(data)\n",
        "\n",
        "lower = df.col0.quantile(.05)\n",
        "upper = df.col0.quantile(.95)\n",
        "\n",
        "df.clip(lower=lower, upper=upper)"
      ],
      "execution_count": 2,
