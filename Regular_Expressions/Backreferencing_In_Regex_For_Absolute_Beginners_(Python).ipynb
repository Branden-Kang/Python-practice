      "source": [
        "import re\n",
        "\n",
        "words = ['apple', 'orange', 'pear', 'happy', 'sad']\n",
        "\n",
        "regex = r'.*([a-zA-Z])\\1.*'\n",
        "\n",
        "matches = [w for w in words if re.match(regex, w)]\n",
        "\n",
        "print(matches)"
      ],
